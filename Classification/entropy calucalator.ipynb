    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "1cf2eceb-d987-45d1-adfb-ae7a4eff7ed7",
   "metadata": {},
   "outputs": [],
   "source": [
    "def entropy(x1,x2):\n",
    "    if x1==0 & x2==0:\n",
    "        return 0\n",
    "    p1=x1/(x1+x2)\n",
    "    p2=x2/(x1+x2)\n",
    "    etp=-(p1*np.log2(p1)+p2*np.log2(p2))\n",
    "    return etp"
   ]
  },
