   "source": [
    "#sum of 2 number is k\n",
    "\n",
    "def sum_ind(lst, k, dic):\n",
    "    for i , n in enumerate(lst):\n",
    "        if k - lst[i] in dic:\n",
    "            return dic[k - lst[i]],i\n",
    "        else:\n",
    "            dic[n] =i\n",
    "    \n",
    "        \n",
    "sum_ind([2,4,5,1], 3, {})   "
